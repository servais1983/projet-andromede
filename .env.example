# Exemple de fichier de variables d'environnement pour Projet Androm\u00e8de
# Copiez ce fichier vers .env et modifiez les valeurs selon votre environnement

# G\u00e9n\u00e9ral
NODE_ENV=development
DEBUG=true

# Base de donn\u00e9es
DB_HOST=postgres
DB_PORT=5432
DB_NAME=andromede_config
DB_USER=andromede
DB_PASSWORD=change_this_in_production

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# API et Auth
API_KEY=dev_api_key_change_in_production
JWT_SECRET=change_this_in_production
ORION_API_KEY=development_key_change_in_production

# Services
SANDBOX_URL=http://neural-sandbox:8080
BLOCKCHAIN_NODE_URL=http://blockchain-node:9050
ORION_CORE_URL=http://orion-core:5000

# Performance
MAX_WORKERS=4
MAX_CONNECTIONS=100
QUEUE_TIMEOUT=300

# Stockage
STORAGE_PATH=/data

# Logging
LOG_LEVEL=debug
LOG_FORMAT=dev

# S\u00e9curit\u00e9
ALLOW_CORS=true
CORS_ORIGIN=http://localhost:3000
RATE_LIMIT=100